<?xml version="1.0"?>
	<!-- Revolute-Revolute Manipulator -->
	<robot name="autolabor_pro1" xmlns:xacro="http://www.ros.org/wiki/xacro">

	<xacro:property name="PI" value="3.1415926535897931"/>
	<xacro:property name="shaft_spacing_f" value="0.19649" />
	<xacro:property name="shaft_spacing_b" value="0.18988" />
	<xacro:property name="wheel_spacing_2" value="0.2643" />
	<xacro:property name="height" value="0.11913" />
	<xacro:arg name="lidar2d" default="fslidar" />
	<xacro:arg name="lidar3d" default="rs16" />
	<xacro:arg name="chassis" default="M1" />
	<!-- Base Link -->
	<link name="base_link">
		<collision>
			<origin xyz="0 0 0.18288" rpy="0 0 0"/>
			<geometry>
				<box size="0.7248 0.4302 0.195"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://autolabor_description/meshes/pro1_body_color.dae"/>
			</geometry>
		</visual>

		<!-- <inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="39"/>
			<inertia ixx="0.7245" ixy="0.0" ixz="0.0" iyy="1.80" iyz="0.0" izz="2.2857"/>
		</inertial> -->
	</link>

	<link name="left_front_wheel">
		<collision>
			<origin xyz="0 0 0" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.0822" radius="0.127"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://autolabor_description/meshes/pro1_wheel_color.dae"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="1.5"/>
			<inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
		</inertial>
	</link>

	<link name="left_back_wheel">
		<collision>
			<origin xyz="0 0 0" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.0822" radius="0.127"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://autolabor_description/meshes/pro1_wheel_color.dae"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="1.5"/>
			<inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
		</inertial>
	</link>

	<link name="right_front_wheel">
		<collision>
			<origin xyz="0 0 0" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.0822" radius="0.127"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 ${PI}"/>
			<geometry>
				<mesh filename="package://autolabor_description/meshes/pro1_wheel_color.dae"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 ${PI}"/>
			<mass value="1.5"/>
			<inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
		</inertial>
	</link>

	<link name="right_back_wheel">
		<collision>
			<origin xyz="0 0 0" rpy="1.5708 0 0"/>
			<geometry>
				<cylinder length="0.0822" radius="0.127"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 ${PI}"/>
			<geometry>
				<mesh filename="package://autolabor_description/meshes/pro1_wheel_color.dae"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 ${PI}"/>
			<mass value="1.5"/>
			<inertia ixx="0.013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.0074"/>
		</inertial>
	</link>


	<link name="lidar_front">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.05" radius="0.03"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.03" radius="0.03"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="0.0074" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.013"/>
		</inertial>
	</link>

	<link name="lidar_back">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.03" radius="0.03"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.03" radius="0.03"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="0.0074" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.013"/>
		</inertial>
	</link>



	<link name="tag">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.05 0.03"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.05 0.03"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="0.0074" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.013"/>
		</inertial>
	</link>

	<link name="imu_link">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.03 0.03"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.05 0.03 0.03"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="0.0013" ixy="0.0" ixz="0.0" iyy="0.0074" iyz="0.0" izz="0.074"/>
		</inertial>
	</link>

	<link name="rslidar">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.07" radius="0.05"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.07" radius="0.05"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="0.012" ixy="0.0" ixz="0.0" iyy="0.012" iyz="0.0" izz="0.02"/>
		</inertial>
	</link>

	<link name="velodyne">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.07" radius="0.05"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder length="0.07" radius="0.05"/>
			</geometry>
		</visual>

		<inertial>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<mass value="0.1"/>
			<inertia ixx="0.012" ixy="0.0" ixz="0.0" iyy="0.012" iyz="0.0" izz="0.02"/>
		</inertial>
	</link>



	<joint name="joint_left_front" type="continuous">
		<parent link="base_link"/>
		<child link="left_front_wheel"/>
		<origin xyz="${shaft_spacing_f} ${wheel_spacing_2} ${height}" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<dynamics damping="0.7"/>
	</joint>

	<joint name="joint_left_back" type="continuous">
		<parent link="base_link"/>
		<child link="left_back_wheel"/>
		<origin xyz="-${shaft_spacing_b} ${wheel_spacing_2} ${height}" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<dynamics damping="0.7"/>
	</joint>

	<joint name="joint_right_front" type="continuous">
		<parent link="base_link"/>
		<child link="right_front_wheel"/>
		<origin xyz="${shaft_spacing_f} -${wheel_spacing_2} ${height}" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<dynamics damping="0.7"/>
	</joint>

	<joint name="joint_right_back" type="continuous">
		<parent link="base_link"/>
		<child link="right_back_wheel"/>
		<origin xyz="-${shaft_spacing_b} -${wheel_spacing_2} ${height}" rpy="0 0 0"/>
		<axis xyz="0 1 0"/>
		<dynamics damping="0.7"/>
	</joint>

	<xacro:if value="${'$(arg lidar2d)' == 'fslidar'}">
		<joint name="laser_front_link_joint" type="fixed">
			<parent link="base_link"/>
			<child link="lidar_front"/>
			<origin xyz="0.4435 0.0 0.15" rpy="3.14 0.0 0.0"/>
		</joint>

		<joint name="laser_back_link_joint" type="fixed">
			<parent link="base_link"/>
			<child link="lidar_back"/>
			<origin xyz="-0.4355 0.0 0.15" rpy="3.14 0.0 3.14"/>
		</joint>
	</xacro:if>


	<xacro:if value="${'$(arg chassis)' == 'AP1'}">
            <xacro:if value="${'$(arg lidar2d)' == 'ldlidar'}">
            		<joint name="laser_front_link_joint" type="fixed">
            			<parent link="base_link"/>
            			<child link="lidar_front"/>
            			<origin xyz="0.4435 0.0 0.15" rpy="3.14 0.0 1.57"/>
            		</joint>

            		<joint name="laser_back_link_joint" type="fixed">
            			<parent link="base_link"/>
            			<child link="lidar_back"/>
            			<origin xyz="-0.4355 0.0 0.15" rpy="3.14 0.0 -1.57"/>
            		</joint>
            </xacro:if>
    </xacro:if>


  	 <xacro:if value="${'$(arg chassis)' == 'M1'}">
              <xacro:if value="${'$(arg lidar2d)' == 'ldlidar'}">
              		<joint name="laser_front_link_joint" type="fixed">
              			<parent link="base_link"/>
              			<child link="lidar_front"/>
              			<origin xyz="0.316 0.0 0.15" rpy="0 0.0 -1.57"/>
              		</joint>

              		<joint name="laser_back_link_joint" type="fixed">
              			<parent link="base_link"/>
              			<child link="lidar_back"/>
              			<origin xyz="-0.316 0.0 0.15" rpy="0 0.0 1.57"/>
              		</joint>
              	</xacro:if>
     </xacro:if>


	<joint name="tag_link_joint" type="fixed">
		<parent link="base_link"/>
		<child link="tag"/>
		<origin xyz="0.09 0.0 0.60"/>
	</joint>

	<joint name="imu_link_joint" type="fixed">
		<parent link="base_link"/>
		<child link="imu_link"/>
		<origin xyz="-0.04 0.0 0.00" rpy="0.0 0.0 0.0"/>
	</joint>

	<joint name="rslidar_link_joint" type="fixed">
		<parent link="base_link"/>
		<child link="rslidar"/>
		<origin xyz="0.12 0.0 0.65" rpy="0.0 0.0 0.0"/>
	</joint>


	<joint name="velodyne_link_joint" type="fixed">
		<parent link="base_link"/>
		<child link="velodyne"/>
		<origin xyz="0.12 0.0 0.65" rpy="0.0 0.0 0.0"/>
	</joint>

</robot>
